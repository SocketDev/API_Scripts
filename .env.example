# Socket API Configuration
# Base URL for the Socket API
SOCKET_API_BASE_URL=https://api.socket.dev/v0

# Authentication
# Your Socket organization token (Bearer token)
SOCKET_API_TOKEN=your_socket_api_token_here

# Organization details
SOCKET_ORG_SLUG=your_organization_slug_here

# Test data
# Sample PURLs for testing package endpoints
TEST_NPM_PACKAGE=pkg:npm/express@4.19.2
TEST_PYPI_PACKAGE=pkg:pypi/django@5.0.6
TEST_MAVEN_PACKAGE=pkg:maven/log4j/log4j@1.2.17

# Repository details for testing
TEST_REPO_SLUG=your_test_repository_slug
TEST_BRANCH=main
TEST_COMMIT_HASH=abc123def456

# License policy test data
TEST_LICENSE_ALLOW=Apache-2.0
TEST_LICENSE_WARN=copyleft

# Alert types for testing
TEST_ALERT_TYPE=malicious-code

# File paths for testing
TEST_MANIFEST_FILE=package.json
TEST_SBOM_FILE=sbom.json

# Rate limiting and quotas
MAX_REQUESTS_PER_MINUTE=100
RETRY_DELAY_SECONDS=60

# Output and logging
LOG_LEVEL=INFO
OUTPUT_DIR=./test_results
